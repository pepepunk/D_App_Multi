<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="D_App_Multi.View.Login">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Correo"/>
            <Entry Text="{Binding usr}"/>
            <Label Text="Contrasena"/>
            <Entry Text="{Binding pass}" IsPassword="True"/>
            <Button Text="Guardar" Command="{Binding IniciarSessionCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>